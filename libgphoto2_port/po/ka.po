#: libgphoto2_port/gphoto2-port.c:687
msgid "low"
msgstr "დაბალი"

#: libgphoto2_port/gphoto2-port.c:688
msgid "high"
msgstr "მაღალი"

#: libgphoto2_port/gphoto2-port.c:1216
